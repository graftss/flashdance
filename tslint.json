{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "array-type": [true, "array"],
      "arrow-parens": [true, "ban-single-arg-parens"],
      "member-ordering": [false],
      "no-console": [false],
      "ordered-imports": [false],
      "quotemark": [true, "single", "avoid-escape", "avoid-template"]
    },
    "rulesDirectory": []
}
